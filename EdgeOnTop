    if game.PlaceId ~= 2788229376 then game.StarterGui:SetCore("SendNotification", {
    Title = "EDGE.CC";
    Text = "Game: Not Supported";
    Duration = 15;
})
  
    wait(3) return end

    if game.Players.LocalPlayer:IsInGroup(13066992) then
	game.StarterGui:SetCore("SendNotification", {
		Title = "EDGE.CC";
		Text = "Edge, You're whitelisted.";
		Icon = "https://tr.rbxcdn.com/5fd82246385ec6eadc5fdfea482be5b9/420/420/Decal/Png";
	})
end
