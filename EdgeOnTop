local amount = 150
 
game:GetService'Workspace'.Camera.FieldOfView = tonumber(object.Slider)
   print(slider)
end})
local Button = ("Autoclicker (Z)")
local time = 0.01 --decrease if too slow increase if too fast
click = false
m = game.Players.LocalPlayer:GetMouse()
m.KeyDown:connect(function(key)
if key == "z" then
if click == true then click = false
elseif
click == false then click = true
while click == true do 
wait(time)
mouse1click()
end
end
end
end)
